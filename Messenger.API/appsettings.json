{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "NotificationsConfiguration": {
    "Email": {
      "Enabled": true,
      "Providers": {
        "Twilio": { "Enabled": true, "Priority": 1 },
        "Vonage": { "Enabled": true, "Priority": 2 },
        "Aws_SNS": { "Enabled": true, "Priority": 3 }
      }
    },
    "Sms": {
      "Enabled": true,
      "Providers": { 
        "Twilio": { "Enabled": true, "Priority": 1 },
        "Vonage": { "Enabled": true, "Priority": 2 }
      }
    }
  }
}